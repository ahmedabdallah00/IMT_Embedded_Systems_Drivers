#include "../STD_TYPES.h"

#include "DIO_interface.h"
#include "DIO_private.h"
#include "DIO_config.h"


void DIO_SETPinDirection(u8 portno , u8 pinno, u8 direction)
{
	switch(portno)
	{
	case DIO_PORTA:
		SET_BIT(DDRA,pinno);
		break;
	case DIO_PORT:
		SET_BIT(DDRA,pinno);
		break;

	}
}
